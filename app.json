{
  "expo": {
    "name": "Recipe E-Book",
    "slug": "AppDev",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/WG-SPLASH.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/WG-SPLASH.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.carlsidy.recipeBoxWG"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/WG-SPLASH.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/WG-SPLASH.png",
          "imageWidth": 200,
          "resizeMode": "cover",
          "backgroundColor": "#ffffff",
          "borderRadius" : "100%"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "64cc8ea2-dbc6-4517-9fcd-629351e6a1cc"
      }
    }
  }
}
